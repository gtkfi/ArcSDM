<metadata xml:lang="en"><Esri><CreaDate>20241120</CreaDate><CreaTime>10222700</CreaTime><ArcGISFormat>1.0</ArcGISFormat><SyncOnce>TRUE</SyncOnce><ModDate>20241122</ModDate><ModTime>8331400</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>ItemDescription</ArcGISProfile></Esri><tool name="TrainMLPClassifierTool" displayname="Train MLP Classifier" toolboxalias="ArcSDM" xmlns=""><arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath><parameters><param name="X" displayname="Input Features" type="Required" direction="Input" datatype="Feature Class or Raster Layer" expression="X"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The input features for the neural network.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="y" displayname="Target Variable" type="Required" direction="Input" datatype="Feature Class or Raster Layer" expression="y"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;P&gt;&lt;SPAN&gt;The target variable for the neural network.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;</dialogReference></param><param name="neurons" displayname="Neurons per Layer. A comma separeted list of integers: e.g. 10,5,10" type="Required" direction="Input" datatype="String" expression="neurons"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A comma-separated list of integers specifying the number of neurons in each layer of the neural network.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="validation_split" displayname="Validation Split" type="Optional" direction="Input" datatype="Double" expression="{validation_split}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The proportion of the data to be used for validation. Default is 0.2.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="validation_data" displayname="Validation Data" type="Optional" direction="Input" datatype="Table View" expression="{validation_data}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The validation data as a table view.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="activation" displayname="Activation Function" type="Required" direction="Input" datatype="String" expression="relu | linear | sigmoid | tanh"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The activation function to use in the neural network. Options are "relu", "linear", "sigmoid", and "tanh". Default is "relu".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="output_neurons" displayname="Output Neurons" type="Required" direction="Input" datatype="Long" expression="output_neurons"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The number of neurons in the output layer. Default is 1.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="last_activation" displayname="Last Layer Activation Function" type="Required" direction="Input" datatype="String" expression="sigmoid | softmax"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The activation function for the last layer. Options are "sigmoid" and "softmax". Default is "sigmoid".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="epochs" displayname="Epochs" type="Required" direction="Input" datatype="Long" expression="epochs"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The number of epochs for training the neural network. Default is 50.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="batch_size" displayname="Batch Size" type="Required" direction="Input" datatype="Long" expression="batch_size"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The batch size for training the neural network. Default is 32.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="optimizer" displayname="Optimizer" type="Required" direction="Input" datatype="String" expression="adam | adagrad | rmsprop | sdg"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The optimizer to use for training. Options are "adam", "adagrad", "rmsprop", and "sdg". Default is "adam".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="learning_rate" displayname="Learning Rate" type="Required" direction="Input" datatype="Double" expression="learning_rate"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The learning rate for the optimizer. Default is 0.001.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="loss_function" displayname="Loss Function" type="Required" direction="Input" datatype="String" expression="binary_crossentropy | categorical_crossentropy"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The loss function to use for training. Options are "binary_crossentropy" and "categorical_crossentropy". Default is "binary_crossentropy".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="dropout_rate" displayname="Dropout Rate" type="Optional" direction="Input" datatype="Double" expression="{dropout_rate}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The dropout rate for regularization.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="early_stopping" displayname="Early Stopping" type="Optional" direction="Input" datatype="Boolean" expression="{early_stopping}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Whether to use early stopping during training.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="es_patience" displayname="Early Stopping Patience" type="Optional" direction="Input" datatype="Long" expression="{es_patience}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The number of epochs with no improvement after which training will be stopped. Default is 5.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="validation_metrics" displayname="Validation Metrics" type="Optional" direction="Input" datatype="String" expression="{accuracy | precision | recall}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The metrics to use for validation. Options are "accuracy", "precision", and "recall". Default is "accuracy".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="random_state" displayname="Random State" type="Optional" direction="Input" datatype="Long" expression="{random_state}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The random seed for reproducibility.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="output_file" displayname="Output Model File" type="Required" direction="Output" datatype="File" expression="output_file"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The file path to save the trained model.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param></parameters></tool><dataIdInfo><idCitation><resTitle>Train MLP Classifier</resTitle></idCitation></dataIdInfo><distInfo><distributor><distorFormat><formatName>ArcToolbox Tool</formatName></distorFormat></distributor></distInfo><mdHrLv><ScopeCd value="005"/></mdHrLv><mdDateSt Sync="TRUE">20241122</mdDateSt></metadata>
